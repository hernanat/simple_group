.navbar
  .navbar-elements
    .navbar-brand
      %span.navbar-brand-logo
        = image_tag("logo.png")
      %span.navbar-brand-name
        = t("branding.organization_name")

    .navbar-links
      %ul
        %li
          %span= link_to t("navigation.links.recent"), "#"
        %li
          %span= link_to t("navigation.links.submit"), "#"
        %li
          %span= link_to t("navigation.links.your_submissions"), "#"
        %li
          %span= link_to t("navigation.links.inbox"), "#"
        %li
          %span= link_to(*users_link_for(current_user))
